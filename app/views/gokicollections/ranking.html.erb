<header>
	<h1>User Ranking</h1>
</header>



<main>
	<div class="rankingpage">
		<ul class="rankingcontent">
			<% @rankers.each_with_index do |ranker, i| %>
				<li class="rankerlist">No.<%= i+1 %>  <u><%= ranker.name %></u>   Score:<%= ranker.max_score %></li>
			<% end %>
		</ul>
	</div>
</main>



<footer>
	<div class="footbar">
		<%= link_to gokicollections_path do %>
			<%= image_tag "gokicollections/new/back.png", :size=>"100x100", :class=>"footbutton" %>
		<% end %>
	</div>
</footer>
